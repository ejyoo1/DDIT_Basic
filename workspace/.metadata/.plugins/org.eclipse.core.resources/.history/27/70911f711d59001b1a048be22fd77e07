package ch7;


public class ImportTest {
	public static void main(String[] args){
	
	}
}

class Product{
	int price;
	int bonusPoint = (int)(price/10.0);
	
	Product(int price){
		this.price = price;
		this.bonusPoint =bonusPoint;
	}
}

class Tv extends Product{
	Tv(){
		super(100);
	}
	
	public String toString(){
		return "Tv";
	}
}

class Computer extends Product{
	Computer(){
		super(200);
	}
	
	public String toString(){
		return "Computer";
	}
}